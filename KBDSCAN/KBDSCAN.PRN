  00A8                  PPI.AR  EQU     10101000B       ;A8H    read from PPI Port A
  00A9                  PPI.BR  EQU     10101001B       ;A9H    read from PPI Port B
  00AA                  PPI.CR  EQU     10101010B       ;AAH    read from PPI Port C
  00A8                  PPI.AW  EQU     10101000B       ;A8H    Write to PPI Port A
  00AA                  PPI.CW  EQU     10101010B       ;AAH    write to PPI Port C
  00AB                  PPI.CM  EQU     10101011B       ;ABH    write to PPI command register
                        
  0000'                 JCKBD@::
                        ;
                        ; Scan keyboard
