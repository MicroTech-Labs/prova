                        ;
                        ;       VDP address definition
                        ;
  0098                  VDP.DRW EQU     10011000B       ;98H    Read/write data VDP
  0099                  VDP.CW  EQU     10011001B       ;99H    write command to VDP
  0099                  VDP.SR  EQU     10011001B       ;99H    read status from VDP
                        ;
  0007                  V.COLR  EQU     7               ;In text mode, foreground and background color
                                                        ;Otherwise background color
                        ;
                        ;       PSG address definition
                        ;
  00A0                  PSG.LW  EQU     10100000B       ;A0H    latch address for PSG
  00A1                  PSG.DW  EQU     10100001B       ;A1H    write data to PSG
  00A2                  PSG.DR  EQU     10100010B       ;A2H    read data from PSG
                        ;
  000E                  PSG.PA  EQU     14              ;Port A of PSG
  000F                  PSG.PB  EQU     15              ;Port B of PSG
                        
                        ;
                        ;       PPI address definition
